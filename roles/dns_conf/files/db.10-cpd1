;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	tfg.domain. root.tfg.domain. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dns.tfg.domain.
2.4.0	IN	PTR	dns.tfg.domain.
2.2.1	IN	PTR	lb.tfg.domain.
